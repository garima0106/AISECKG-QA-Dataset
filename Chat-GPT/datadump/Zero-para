    },
    {
      "question": "What is manual code review?",
      "answer": "Manual code review is a technique that involves a thorough manual examination of source code or configuration files to identify coding flaws and security vulnerabilities."
    },
    {
      "question": "What is Static Application Security Testing (SAST)?",
      "answer": "Static Application Security Testing (SAST) is a technique that uses specialized tools to analyze source code or compiled binaries to identify potential vulnerabilities."
    },
    {
      "question": "What is Dynamic Application Security Testing (DAST)?",
      "answer": "Dynamic Application Security Testing (DAST) is a technique that involves testing an application while it is running to simulate real-world attacks and identify vulnerabilities."
    },
    {
      "question": "What is penetration testing?",
      "answer": "Penetration testing is a technique that simulates real-world attacks to identify vulnerabilities by conducting controlled attacks on systems, networks, or applications."
    },
    {
      "question": "What is vulnerability scanning?",
      "answer": "Vulnerability scanning is a technique that uses automated tools to scan networks, systems, or applications to identify known vulnerabilities."
    },
    {
      "question": "What is fuzz testing?",
      "answer": "Fuzz testing is a technique that involves sending unexpected or random data inputs to an application or system to discover vulnerabilities caused by input handling errors."
    },
    {
      "question": "What is security code review?",
      "answer": "Security code review is a technique that focuses on identifying security vulnerabilities in the codebase by examining the source code for insecure coding practices or access control issues."
    },
    {
      "question": "What are security audits?",
      "answer": "Security audits are comprehensive reviews of systems, networks, or applications to assess their security posture and identify vulnerabilities."
    },
    {
      "question": "What is web application security testing?",
      "answer": "Web application security testing is a technique that focuses on assessing the security of web applications by testing aspects such as input validation, authentication mechanisms, and secure communication."
    }
  ]
